# Fügt fehlende Formschlagwörter für Aufsatzsammlungen den MARC-Daten hinzu.

if marc_match(LDR/7,'m')
   unless marc_match(008/30,'1')
      unless marc_match(008/29,'1')
	     unless marc_match(655$0,"\(DE\-588\)4143413\-4")
            marc_add(655,a,'Aufsatzsammlung',0,'(DE-588)4143413-4')
         end
	  end
   end
end